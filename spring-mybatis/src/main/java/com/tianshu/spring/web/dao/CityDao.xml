<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tianshu.spring.web.dao.CityDao">
    <select id="selectCityByPid" resultType="com.tianshu.spring.web.domain.City">
        SELECT id,name,pid FROM city WHERE pid = #{pid}
    </select>
</mapper>
